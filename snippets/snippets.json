{
    "autocmd (Filetype)": {
        "body": [
            "vim.api.nvim_create_autocmd(\"FileType\", {",
            "\tpattern = \"${1:ft}\",",
            "\tcallback = function()",
            "\t\t$0",
            "\tend,",
            "})"
        ],
        "prefix": "autocmd (Filetype)"
    },
    "file exists": {
        "body": "local fileExists = vim.uv.fs_stat(\"${1:filepath}\") ~= nil",
        "prefix": "file exists"
    },
    "feng": {
        "body": "fengscarleteyes",
        "prefix": "feng"
    },
    "disable-line": {
        "body": "---@diagnostic disable-line: rulename",
        "prefix": [
            "disable-line",
            "lua"
        ]
    },
    "disable-next-line": {
        "body": "---@diagnostic disable-next-line: rulename",
        "prefix": [
            "disable-next-line",
            "lua"
        ]
    }
}